{
    "name": "lagdo/dbadmin-app-mono",
    "description": "Monorepo for the Jaxon DbAdmin Laravel applications",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thierry Feuzeu"
        }
    ],
    "require": {
        "php": "^8.2",
        "backpack/crud": "^7.0",
        "backpack/theme-tabler": "^2.0",
        "infisical/php-sdk": "^0.0.1",
        "jaxon-php/jaxon-laravel": "^5.0",
        "jaxon-php/jaxon-upload": "^2.1",
        "lagdo/dbadmin-driver-mysql": "0.22.*",
        "lagdo/dbadmin-driver-pgsql": "0.22.*",
        "lagdo/dbadmin-driver-sqlite": "0.22.*",
        "lagdo/jaxon-dbadmin": "0.29.*",
        "lagdo/laravel-facades": "^1.0",
        "lagdo/ui-builder-bootstrap5": "0.5.*",
        "laravel/fortify": "^1.28",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1"
    },
    "require-dev": {
        "backpack/generators": "^4.1",
        "contao/monorepo-tools": "^0.2",
        "fakerphp/faker": "^1.23",
        "jeroen-g/laravel-packager": "^2.11",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "replace": {
        "lagdo/dbadmin-app": "self.version",
        "lagdo/dbadmin-backpack-addon": "self.version",
        "laravel/laravel": "self.version"
    },
    "autoload": {
        "psr-4": {
            "App\\": "backpack/app/",
            "Database\\Factories\\": "backpack/database/factories/",
            "Database\\Seeders\\": "backpack/database/seeders/",
            "Lagdo\\Dbadmin\\Backpack\\": "backpack-addon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lagdo\\Dbadmin\\Backpack\\Tests\\": "backpack-addon/tests",
            "Tests\\": "backpack/tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": []
    },
    "minimum-stability": "stable"
}
